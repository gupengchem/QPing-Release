<div class="default-hidden">
    <form class="form-horizontal edit-form" id="update-password-form">
        <input class="form-control" name="wechat_id" type="hidden">
        <!--<input type="hidden" class="form-control" name="_id">-->
        <div class="form-group">
            <label class="col-md-3 control-label">旧密码</label>
            <div class="col-md-7">
                <input class="form-control" name="oldPassword" type="password" dol-validate="required">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-3 control-label">新密码</label>
            <div class="col-md-7">
                <input class="form-control" name="newPassword" type="password" dol-validate="required" id="newPassword">
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-3 control-label">确认密码</label>
            <div class="col-md-7">
                <input class="form-control" name="confirmPassword" type="password" dol-validate='required,sameInput[#newPassword|新密码]'>
            </div>
        </div>
    </form>
    <div id="update_password_form_footer">
        <button type="button" class="btn btn-primary btn-small" id="update_password_form_save">保存</button>
        <button type="button" class="btn btn-default btn-small dol-hideModal" id="edit_form_cancel">取消</button>
    </div>
</div>
